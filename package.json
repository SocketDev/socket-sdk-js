{
  "name": "@socketsecurity/sdk",
  "version": "1.4.6",
  "license": "MIT",
  "description": "SDK for the Socket API client",
  "author": {
    "name": "Socket Inc",
    "email": "eng@socket.dev",
    "url": "https://socket.dev"
  },
  "homepage": "https://github.com/SocketDev/socket-sdk-js",
  "repository": {
    "type": "git",
    "url": "git://github.com/SocketDev/socket-sdk-js.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:0": "run-s clean",
    "build:1-declaration": "tsc",
    "build:2-esm": "tsc -p tsconfig.esm.json",
    "build": "run-s build:*",
    "check": "run-p -c --aggregate-output check:*",
    "check:lint": "eslint --report-unused-disable-directives .",
    "check:lint:fix": "npm run check:lint -- --fix",
    "check:tsc": "tsc",
    "coverage": "run-s coverage:*",
    "coverage:test": "run-s test:prepare test:unit:coverage",
    "coverage:type": "type-coverage --detail",
    "ci-test": "run-s build test:*",
    "clean": "run-p clean:*",
    "clean:declarations": "del-cli '*.d.ts' '!api*.d.ts'",
    "fix": "run-s lint:fix check:lint:fix",
    "generate-sdk:0-prettify": "node build/prettify-base-json.js",
    "generate-sdk:1-generate": "node build/generate-types.js > types/api.d.ts",
    "generate-sdk": "run-s generate-sdk:*",
    "knip:dependencies": "knip --dependencies",
    "knip:exports": "knip --include exports,duplicates",
    "lint": "oxlint -c=./.oxlintrc.json --ignore-path=./.oxlintignore --tsconfig=./tsconfig.json .",
    "lint:fix": "npm run lint -- --fix && npm run lint:fix:fast",
    "lint:fix:fast": "biome format --write",
    "lint-staged": "lint-staged",
    "precommit": "lint-staged",
    "prepare": "husky",
    "prepublishOnly": "run-s build",
    "test:mocha": "c8 --reporter=lcov --reporter text mocha 'test/**/*.spec.js'",
    "test": "run-s check test:*",
    "update": "run-p --aggregate-output update:**",
    "update:deps": "npx --yes npm-check-updates"
  },
  "dependencies": {
    "formdata-node": "^6.0.3",
    "got": "^14.4.6",
    "pony-cause": "^2.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.22.0",
    "@socketsecurity/registry": "1.0.132",
    "@types/chai": "^5.2.0",
    "@types/chai-as-promised": "^8.0.2",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "c8": "^10.1.3",
    "chai": "^5.2.0",
    "chai-as-promised": "^8.0.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.22.0",
    "eslint-import-resolver-oxc": "^0.13.1",
    "eslint-plugin-import-x": "^4.8.0",
    "eslint-plugin-jsdoc": "^50.6.7",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "knip": "^5.46.0",
    "lint-staged": "^15.5.0",
    "mocha": "^11.1.0",
    "nock": "^14.0.1",
    "npm-run-all2": "^7.0.2",
    "openapi-typescript": "^7.6.1",
    "oxlint": "0.16.0",
    "type-coverage": "^2.29.7",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "engines": {
    "node": "^18.20.7 || ^20.18.3 || >=22.14.0"
  },
  "files": [
    "dist/**",
    "types/**"
  ],
  "lint-staged": {
    "*.{cjs,js,json,md,mjs,mts,ts}": [
      "npm run lint -- --fix",
      "npm run lint:fix:fast -- --no-errors-on-unmatched --files-ignore-unknown=true --colors=off"
    ]
  }
}

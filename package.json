{
  "name": "@socketsecurity/sdk",
  "version": "0.2.1",
  "license": "MIT",
  "description": "SDK for the Socket API client",
  "author": {
    "name": "Socket Inc",
    "email": "eng@socket.dev",
    "url": "https://socket.dev"
  },
  "homepage": "https://github.com/SocketDev/socket-sdk-js",
  "repository": {
    "type": "git",
    "url": "git://github.com/SocketDev/socket-sdk-js.git"
  },
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts.map",
    "index.d.ts",
    "index.js",
    "lib/**/*.d.ts.map",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "types/**/*.d.ts"
  ],
  "dependencies": {
    "formdata-node": "^5.0.0",
    "got": "^12.5.2",
    "pony-cause": "^2.1.4"
  },
  "devDependencies": {
    "@socketsecurity/eslint-config": "^1.0.0",
    "@tsconfig/node14": "^1.0.3",
    "@types/node": "^14.18.31",
    "@types/tap": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.9",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nock": "^13.2.9",
    "npm-run-all2": "^6.0.2",
    "openapi-typescript": "^5.4.1",
    "tap": "^16.3.0",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build:0": "run-s clean",
    "build:1-declaration": "tsc -p declaration.tsconfig.json",
    "build": "run-s build:*",
    "ci-test": "run-s build test:*",
    "clean:declarations": "rm -rf $(find . -maxdepth 2 -type f -name '*.d.ts*' ! -name 'api*.d.ts')",
    "clean": "run-p clean:*",
    "generate-sdk:types": "node build/generate-types.js > types/api.d.ts",
    "generate-sdk": "run-s generate-sdk:*",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "test:tap": "tap --no-check-coverage",
    "test": "run-s lint build test:*"
  }
}

{
  "root": true,
  "extends": [
    "@socketsecurity/eslint-config/jsdoc"
  ],
  "settings": {
    "jsdoc": {
      "mode": "typescript"
    }
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/quotes": ["error", "single", { "avoidEscape": true, "allowTemplateLiterals": false }],
    "no-console": "warn"
  }
}

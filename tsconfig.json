{
  "extends": "./.config/tsconfig.base.json",
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "bundler",
    "outDir": "./dist",
    "rootDir": "./src",
    "noPropertyAccessFromIndexSignature": false,
    "noEmit": true,
    "paths": {
      "@socketsecurity/registry/*": [
        "../socket-registry/registry/dist/*",
        "./node_modules/@socketsecurity/registry/*"
      ]
    }
  },
  "include": ["src/**/*.ts"]
}